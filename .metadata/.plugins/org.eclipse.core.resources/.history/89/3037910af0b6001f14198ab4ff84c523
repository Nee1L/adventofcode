//		DayTwo.readFile();
		// DayThree.readFile();
		// DayFour.readFile();
		String buff = "";
		ArrayList<ArrayList<Integer>> arr = new ArrayList<>();
		for (int k = -6; k < 6; k++) {
			buff = "";
			for (int i = 0; i < 6; i++) {
				for (int j = 0; j < 6; j++) {
					if (i == j - k) {
						buff = buff + "1";
						// continue;
					}
					// System.out.print(0);
				}
			}
			System.out.println(buff);
		}
	}

