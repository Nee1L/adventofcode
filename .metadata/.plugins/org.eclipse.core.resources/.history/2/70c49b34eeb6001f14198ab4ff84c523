package adventofcode2;

public class AdventOfCode {

	public static void main(String[] args) {

		// DayOne.readFile();
//		ArrayList<String> asd = new ArrayList<>();
//		asd.add("wqeqw");
//		System.out.println(asd.toString());
//		DayTwo.readFile();
		// DayThree.readFile();
		// DayFour.readFile();
		for (int k = -6; k < 6; k++) {
			for (int i = 0; i < 6; i++) {
				for (int j = 0; j < 6; j++) {
					if (i == j - k) {
						System.out.print(1);
						continue;
					}
					System.out.print(0);
				}
				System.out.println();
			}
		}
	}

}
